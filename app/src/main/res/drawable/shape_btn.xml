<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle"  >
            <solid android:color="#123b00"/>
            <corners android:radius="20dp" />
            <stroke android:color="#d4d4d4" android:dashGap="1dp" android:dashWidth="10dp" android:width="5dp"/>
        </shape>

    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle"  >
            <solid android:color="#d4d4d4"/>
            <corners android:radius="20dp" />
            <stroke android:color="#3d3d3d" android:dashGap="1dp" android:dashWidth="10dp" android:width="5dp"/>
        </shape>
    </item>
</selector>